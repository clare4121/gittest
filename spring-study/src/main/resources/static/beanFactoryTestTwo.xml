<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd" >
    <bean id="getBeanTest" class="com.cyp.springstudy.defaultListableBeanFactoryStudy.test.GetBeanTest">
        <lookup-method name="getBean" bean="student"/>
    </bean>
    <bean id="teacher" class="com.cyp.springstudy.defaultListableBeanFactoryStudy.mybean.Teacher"></bean>
    <bean id="student" class="com.cyp.springstudy.defaultListableBeanFactoryStudy.mybean.Student"></bean>
</beans>